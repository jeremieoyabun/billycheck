generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Scan {
  id            String   @id @default(cuid())
  createdAt     DateTime @default(now())
  status        String
  engagement    String?
  fileKey       String?
  originalName  String?
  mimeType      String?
  size          Int?
  resultJson    Json?
}
